{
  "name": "@gmod/cram",
  "version": "0.0.1",
  "description": "read CRAM files with pure Javascript",
  "license": "MIT",
  "repository": "GMOD/cram-js",
  "main": "dist/index.js",
  "author": {
    "name": "Robert Buels",
    "email": "rbuels@gmail.com",
    "url": "https://github.com/rbuels"
  },
  "engines": {
    "node": ">=6"
  },
  "bin": {},
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha",
    "test-browser": "karma start --single-run",
    "coverage": "rm -rf coverage; nyc mocha; nyc report --reporter=lcov",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "docs": "documentation readme --shallow src/indexedCramFile.js --section=IndexedCramFile; documentation readme --shallow src/cramFile/file.js --section=CramFile; documentation readme --shallow src/cramIndex.js --section=CraiIndex;  documentation readme --shallow src/errors.js '--section=Exception Classes'; documentation readme --shallow src/cramFile/file.js --section=CramFile; documentation readme --shallow src/cramIndex.js --section=CraiIndex;  documentation readme --shallow src/cramFile/record.js --section=CramRecord",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "babel src -d dist",
    "watch": "npm-watch",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "git push origin master --follow-tags"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "keywords": [
    "cram",
    "genomics",
    "bionode",
    "biojs"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "buffer-crc32": "^0.2.13",
    "cross-fetch": "^2.1.0",
    "es6-promisify": "^6.0.0",
    "long": "^4.0.0",
    "lru-cache": "^4.1.3",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^4.1.2",
    "documentation": "^5.3.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "get-stream": "^3.0.0",
    "glob": "^7.1.2",
    "imports-loader": "^0.8.0",
    "istanbul": "^0.4.5",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.1.1",
    "npm-watch": "^0.3.0",
    "nyc": "^13.0.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.1",
    "webpack": "4",
    "webpack-cli": "^2.0.15"
  }
}
